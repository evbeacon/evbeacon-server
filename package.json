{
  "name": "evbeacon-server",
  "version": "0.1.0",
  "description": "Backend server for EVBeacon",
  "author": "Josh McFarlin <jmcfarlin6@gatech.edu> (https://mcfarl.in/)",
  "license": "GPL-3.0",
  "keywords": [
    "evbeacon",
    "ev"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evbeacon/evbeacon-server.git"
  },
  "bugs": {
    "url": "https://github.com/evbeacon/evbeacon-server/issues"
  },
  "homepage": "https://github.com/evbeacon/evbeacon-server#readme",
  "lint-staged": {
    "*.{ts,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "update-env": "vercel env pull",
    "deploy": "vercel",
    "deploy-prod": "vercel --prod",
    "format": "prettier --write **/*.{ts,json}",
    "lint": "eslint --fix --ext ts .",
    "test": "jest --runInBand",
    "test-coverage": "jest --runInBand --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "dayjs": "^1.8.36",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.5",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@tsconfig/node12": "^1.0.7",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.28",
    "@types/mongoose": "^5.7.36",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@vercel/node": "^1.8.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-typescript": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "12.x"
  }
}
